    "typecheck": "tsgo --noEmit",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "pako": "^2.1.0",
    "ulidx": "^2.4.1"
  },
  "peerDependencies": {
    "react-native": ">=0.70.0",
    "react": ">=18.0.0",
    "@react-native-community/netinfo": ">=11.0.0",
    "expo-file-system": ">=17.0.0",
    "expo-device": ">=6.0.0",
    "expo-localization": ">=14.0.0",
    "expo-router": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "@react-native-community/netinfo": {
      "optional": true
    },
    "expo-file-system": {
      "optional": true
    },
    "expo-device": {
      "optional": true
    },
    "expo-localization": {
      "optional": true
    },
    "expo-router": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/pako": "^2.0.4",
    "@types/react": "^19.1.8",
    "expo-file-system": "^17.0.1",
    "expo-router": "^6.0.19",
    "tsup": "^8.3.5",
    "typescript": "^5",
    "@typescript/native-preview": "^7.0.0-dev.20251214.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phase-Analytics/Phase.git"
  },
  "homepage": "https://phase.sh",
  "bugs": {
    "url": "https://github.com/Phase-Analytics/Phase/issues"
